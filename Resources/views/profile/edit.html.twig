{% extends '@ScyLabsUser/layout.html.twig' %}
{% trans_default_domain 'ScyLabsUserBundle' %}
{% block content_profile %}
    {{ ScyLabs_showHook(_self,'scy_labs.user.edit.before_form') | raw }}
    {{ form_start(form) }}
    {{ form_rest(form) }}
    {{ ScyLabs_showHook(_self,'scy_labs.user.edit.before_submit') | raw }}
    <button type="submit">{{ 'form.send_edit' | trans }}</button>
    {{ form_end(form) }}
    {{ ScyLabs_showHook(_self,'scy_labs.user.edit.after_form') | raw }}

{% endblock %}
{% block css %}
    {{ parent() }}
    {{ ScyLabs_showHook(_self,'scy_labs.user.edit.head_assets') | raw }}
{% endblock css %}
{% block afterScriptLoader %}
    {{ ScyLabs_showHook(_self,'scy_labs.user.edit.after_script_loader') | raw }}
{% endblock %}